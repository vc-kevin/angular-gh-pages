{
  "name": "calopad-cards-widget",
  "version": "1.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "echo \"Building development app\" && ng build --configuration=development && npm run gulp-add-pre-compiled-scripts",
    "build:prod": "echo \"Building production app\" && ng build --configuration=production && npm run gulp-add-pre-compiled-scripts",
    "gulp-add-pre-compiled-scripts": "echo \"Executing gulp command to add pre compiled scripts into widget.html ...\" && gulp --color add-compiled-scripts-to-widget-html",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.4",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.4",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ng-select/ng-select": "^10.0.3",
    "@ngneat/until-destroy": "^9.2.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "crypto-js": "^4.1.1",
    "intl-tel-input": "^17.0.19",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.1",
    "@angular/cli": "~15.0.1",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.4",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2"
  }
}
